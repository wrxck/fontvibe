{
  "name": "vite-plugin-fontvibe",
  "version": "0.1.0",
  "description": "Vite plugin that auto-injects FontVibe panel in dev mode",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external vite --external @matthesketh/fontvibe"
  },
  "peerDependencies": {
    "vite": "7.3.1",
    "@matthesketh/fontvibe": "0.1.2"
  },
  "author": "Matt Hesketh <matt@heskethwebdesign.co.uk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wrxck/fontvibe.git",
    "directory": "packages/vite-plugin-fontvibe"
  },
  "devDependencies": {
    "tsup": "8.5.1",
    "typescript": "5.7.3"
  }
}
